        "/info": {
            "summary": "Path used to manage the list of infos.",
            "description": "The REST endpoint/path used to list zero or more `info` entities.  This path contains a `GET` operation to perform the list.",
            "get": {
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/info"
                                },
                                "examples": {
                                    "info-example": {
                                        "value": {
                                            "date": "{{ now(yyyyMMdd-HHmmss) }}",
                                            "uptime": "{{ randomInt(1000, 8000) }}"
                                        }
                                    },
                                    "default": {
                                        "value": {
                                            "date": "2018-02-10T09:30Z",
                                            "uptime": 8
                                        }
                                    }
                                }
                            }
                        },
                        "description": "Successful response - returns an `info` entity."
                    }
                },
                "operationId": "getinfo",
                "summary": "Get Info",
                "description": "Gets a list of all `info` entities."
            }
        },
